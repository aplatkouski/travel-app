{
  "presets": [
    [
      "@babel/preset-env",
      {
        "loose": true,
        "corejs": "3.9.0",
        "useBuiltIns": "entry",
        "targets": {
          "browsers": "last 2 chrome versions"
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-proposal-export-default-from",
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ]
  ]
}
